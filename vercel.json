{
  "version": 2,
  "builds": [
    {
      "src": "frontend/package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/frontend/$1"
    }
  ],
  "installCommand": "npm install --legacy-peer-deps",
  "buildCommand": "npm install --legacy-peer-deps --prefix frontend && npm run build --prefix frontend",
  "outputDirectory": "frontend/.next",
  "rootDirectory": "."
}